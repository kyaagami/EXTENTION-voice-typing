{
  "manifest_version": 3,
  "name": "Voice Typing Extension",
  "version": "1.0",
  "description": "Ekstensi untuk pengetikan menggunakan suara",
  "permissions": [
    "activeTab", 
    "scripting", 
    "sidePanel",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}